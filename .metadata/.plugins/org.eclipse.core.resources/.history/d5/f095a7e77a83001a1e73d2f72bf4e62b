package dev.wsd.enums;

import java.util.HashMap;
import java.util.Map;

public enum REIMBURSEMENT_STATUS {
 
	Submitted(1),
	Approved(2),
	Denied(3);

    private int value;
	private static Map map = new HashMap<>();

    private REIMBURSEMENT_STATUS(int value) {
        this.value = value;
    }

    static {
        for (REIMBURSEMENT_STATUS pageType : REIMBURSEMENT_STATUS.values()) {
            map.put(pageType.value, pageType);
        }
    }

    public static REIMBURSEMENT_STATUS valueOf(int pageType) {
        return (REIMBURSEMENT_STATUS) map.get(pageType);
    }

    public int getValue() {
        return value;
    }
}
